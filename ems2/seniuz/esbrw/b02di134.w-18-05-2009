&ANALYZE-RESUME

&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE pi-ped-cancelado B-table-Win 
PROCEDURE pi-ped-cancelado :
/*------------------------------------------------------------------------------
  Purpose:     
  Parameters:  <none>
  Notes:       
------------------------------------------------------------------------------*/
 ASSIGN fi-total-1 = 0
        fi-total-2 = 0
        fi-total-3 = 0
        fi-total-4 = 0.

 IF l-avalia-credito = YES  THEN DO: /* Credito n∆o Aprovados */ 

    ASSIGN c-avalia-credito = "".
    IF l-nao-avaliado = YES THEN
       ASSIGN c-avalia-credito = c-avalia-credito + "1,".
    IF l-nao-aprovado = YES THEN
       ASSIGN c-avalia-credito = c-avalia-credito + "4,".
    IF l-pendente = YES THEN
       ASSIGN c-avalia-credito = c-avalia-credito + "5".

    FOR EACH ped-venda USE-INDEX ch-pre-fat WHERE 
             ped-venda.dt-entrega   >= da-dt-entrega-ini                 AND
             ped-venda.dt-entrega   <= da-dt-entrega-fin                 AND
             INDEX(c-avalia-credito, STRING(ped-venda.cod-sit-aval)) > 0 AND
             ped-venda.nome-abrev   >= c-nome-abrev-ini                  AND
             ped-venda.nome-abrev   <= c-nome-abrev-fin                  AND
             ped-venda.nr-pedcli    >= c-nr-pedcli-ini                   AND
             ped-venda.nr-pedcli    <= c-nr-pedcli-fin                   AND
             ped-venda.no-ab-reppri >= c-no-ab-reppri-ini                AND
             ped-venda.no-ab-reppri <= c-no-ab-reppri-fin 
             NO-LOCK
       BREAK BY ped-venda.dt-entrega
             BY ped-venda.nr-pedcli.

        /* Somente Pedidos (ABERTO, ATENDIDO PARCIAL OU SUSPENSO) */
        IF INDEX("1,2,5", STRING(ped-venda.cod-sit-ped)) =  0 THEN NEXT. 

        IF c-tp-pedido <> 'Todos' THEN DO.
           FIND ped-venda-ext WHERE
                ped-venda-ext.nr-pedido = ped-venda.nr-pedido NO-LOCK NO-ERROR.
           IF NOT AVAIL ped-venda-ext OR
              ped-venda-ext.tp-pedido <> c-tp-pedido THEN NEXT.
        END.
        
        RUN pi-acompanhar IN h-acomp (INPUT "Data: "    + STRING(ped-venda.dt-entrega) +
                                            " Pedido: " + ped-venda.nr-pedcli).

        ASSIGN de-total = 0.
        FOR EACH ped-item OF ped-venda WHERE
                 ped-item.it-codigo >= c-it-codigo-ini AND
                 ped-item.it-codigo <= c-it-codigo-fin AND
                 ped-item.cod-refer >= c-cod-refer-ini AND
                 ped-item.cod-refer <= c-cod-refer-fin NO-LOCK.

            /* SOMENTE ITENS EM ABERTO OU SUSPENSO */
            IF INDEX("1,5", STRING(ped-item.cod-sit-item)) = 0 THEN NEXT. 
            ASSIGN de-total = de-total + ped-item.qt-pedida.

        END.

        IF de-total = 0  THEN NEXT.

        CREATE tt-work.
        ASSIGN tt-work.nr-pedcli-ped = ped-venda.nr-pedcli
               tt-work.nome-ab-cli   = ped-venda.nome-abrev 
               tt-work.no-ab-reppri  = ped-venda.no-ab-reppri   
               tt-work.dt-entrega    = ped-venda.dt-implant  
               tt-work.dt-cancel-ped = ped-venda.dt-entrega 
               tt-work.qtd-ped       = de-total
               tt-work.motivo        = ped-venda.desc-bloq-c.

        ASSIGN fi-total-1 = fi-total-1 + de-total. 
    END.
 END.
 ELSE DO: /* Pedidos Cancelados */

      /* A Data de Cancelamento Pedido pode ser   */
      /* TODAS as datas ou a data de cancelamento */
      ASSIGN da-dt-can-ini = 01/01/0001
             da-dt-can-fin = 12/31/9999.
      IF l-dt-cancela = YES THEN
         ASSIGN da-dt-can-ini = da-dt-cancela-ini
                da-dt-can-fin = da-dt-cancela-fin.

    FOR EACH ped-venda USE-INDEX ch-pre-fat WHERE 
             ped-venda.dt-entrega   >= da-dt-entrega-ini   AND
             ped-venda.dt-entrega   <= da-dt-entrega-fin   AND
             ped-venda.nr-pedcli    >= c-nr-pedcli-ini    AND
             ped-venda.nr-pedcli    <= c-nr-pedcli-fin    AND
             ped-venda.nome-abrev   >= c-nome-abrev-ini   AND
             ped-venda.nome-abrev   <= c-nome-abrev-fin   AND
             ped-venda.no-ab-reppri >= c-no-ab-reppri-ini AND
             ped-venda.no-ab-reppri <= c-no-ab-reppri-fin 
             NO-LOCK
       BREAK BY ped-venda.dt-entrega
             BY ped-venda.nr-pedcli.

        IF c-tp-pedido <> 'Todos' THEN DO.
           FIND ped-venda-ext WHERE
                ped-venda-ext.nr-pedido = ped-venda.nr-pedido NO-LOCK NO-ERROR.
           IF NOT AVAIL ped-venda-ext OR
              ped-venda-ext.tp-pedido <> c-tp-pedido THEN NEXT.
        END.

        RUN pi-acompanhar IN h-acomp (INPUT "Data: "    + STRING(ped-venda.dt-entrega) +
                                            " Pedido: " + ped-venda.nr-pedcli).


        ASSIGN l-achei-item-can = NO.
        IF CAN-FIND(FIRST ped-item OF ped-venda WHERE
                          ped-item.cod-sit-item  = 6             AND
                          ped-item.dt-canseq    >= da-dt-can-ini AND
                          ped-item.dt-canseq    <= da-dt-can-fin) THEN
           ASSIGN l-achei-item-can = YES.

        IF l-dt-cancela             = NO  AND 
           l-cancelados             = YES AND
           ped-venda.cod-sit-ped   <> 6   AND 
                l-achei-item-can    = NO  THEN NEXT.

        IF (l-dt-cancela = YES AND l-achei-item-can = NO)  AND
           (ped-venda.dt-cancela < da-dt-cancela-ini  OR
            ped-venda.dt-cancela > da-dt-cancela-fin) THEN NEXT.

        IF (l-cancelados = YES AND l-achei-item-can = NO)  AND
           ped-venda.cod-sit-ped <> 6 THEN DO:
           NEXT.
        END.

        ASSIGN de-total     = 0
               c-motivo     = ""
               da-dt-cancel = ped-venda.dt-cancela.
        FOR EACH ped-item OF ped-venda WHERE
                 ped-item.cod-sit-item = 6                      AND
                 ped-item.dt-canseq   >= da-dt-can-ini          AND
                 ped-item.dt-canseq   <= da-dt-can-fin          AND 
                 ped-item.it-codigo   >= c-it-codigo-ini AND
                 ped-item.it-codigo   <= c-it-codigo-fin AND
                 ped-item.cod-refer   >= c-cod-refer-ini AND
                 ped-item.cod-refer   <= c-cod-refer-fin NO-LOCK.

             ASSIGN de-total = de-total + ped-item.qt-pedida.

             IF c-motivo = "" THEN
                ASSIGN c-motivo     = ped-item.desc-cancela
                       da-dt-cancel = ped-item.dt-canseq.
        END.

        IF de-total = 0  THEN NEXT.

        CREATE tt-work.
        ASSIGN tt-work.nr-pedcli-ped = ped-venda.nr-pedcli
               tt-work.nome-ab-cli   = ped-venda.nome-abrev 
               tt-work.no-ab-reppri  = ped-venda.no-ab-reppri   
               tt-work.dt-entrega    = ped-venda.dt-entrega  
               tt-work.dt-cancel-ped = da-dt-cancel 
               tt-work.qtd-ped       = de-total
               tt-work.motivo        = c-motivo.

        ASSIGN fi-total-1 = fi-total-1 + de-total. 
        
    END.
 END.
 DISPLAY fi-total-1
         WITH FRAME {&FRAME-NAME}.



END PROCEDURE.

