/* ************************  Control Triggers  ************************ */

&Scoped-define BROWSE-NAME br-work
&Scoped-define SELF-NAME br-work
&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL br-work B-table-Win
ON VALUE-CHANGED OF br-work IN FRAME F-Main
DO:
  DISABLE {&list-6} WITH FRAME {&FRAME-NAME}.
  DISABLE {&list-4} WITH FRAME {&FRAME-NAME}.
  IF AVAIL tt-work THEN DO:
     CASE i-tp-relat:
         WHEN 1 THEN DO:
             bt-pre-nota:SENSITIVE IN FRAME {&FRAME-NAME}  = YES.
             bt-pedido:SENSITIVE IN FRAME {&FRAME-NAME}    = YES.
         END.
         WHEN 2 THEN DO:
             bt-nota:SENSITIVE IN FRAME {&FRAME-NAME} = YES.
         END.
         WHEN 3 THEN DO:
             bt-devolucao:SENSITIVE IN FRAME {&FRAME-NAME} = YES.  /* Rotina n∆o Consegue Posicionar na NF */
             bt-item:SENSITIVE IN FRAME {&FRAME-NAME} = YES.
         END.
     END CASE.
     ENABLE {&list-6} WITH FRAME {&FRAME-NAME}.
  END.
END.

/* _UIB-CODE-BLOCK-END */
&ANALYZE-RESUME


&Scoped-define SELF-NAME bt-devolucao
&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL bt-devolucao B-table-Win
ON CHOOSE OF bt-devolucao IN FRAME F-Main
DO:
  
  FIND FIRST devol-cli WHERE
             devol-cli.cod-estabel = '2' AND
             devol-cli.serie       = '1' AND
             devol-cli.nr-nota-fis = tt-work.nr-nota-fis NO-LOCK NO-ERROR.
  IF AVAIL devol-cli THEN DO:
     FIND emitente WHERE
          emitente.cod-emit = devol-cli.cod-emit NO-LOCK NO-ERROR.
     FIND docum-est WHERE
          docum-est.serie-docto  = devol-cli.serie-docto  AND
          docum-est.nro-docto    = devol-cli.nro-docto    AND
          docum-est.cod-emitente = devol-cli.cod-emitente AND
          docum-est.nat-operacao = devol-cli.nat-operacao NO-LOCK NO-ERROR.
     IF AVAIL docum-est THEN DO:
        ASSIGN gr-emitente  = ROWID(emitente)
               gr-docum-est = ROWID(docum-est).

        RUN rep/re0701a.p.
     END.
  END.
  
END.

/* _UIB-CODE-BLOCK-END */
&ANALYZE-RESUME


&Scoped-define SELF-NAME bt-excel
&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL bt-excel B-table-Win
ON CHOOSE OF bt-excel IN FRAME F-Main /* Button 2 */
DO:
  
   RUN esdlg/d02essp0164.w (INPUT i-tp-relat, OUTPUT arq-saida).
   IF arq-saida <> "" THEN DO:
      RUN pi-gera-excel (INPUT arq-saida).
      MESSAGE "O arquivo foi gerado em " TRIM(arq-saida) + "." SKIP 
              "Para acess†-lo,  abra-o atravÇs do Excel."
          VIEW-AS ALERT-BOX INFO BUTTONS OK. 
