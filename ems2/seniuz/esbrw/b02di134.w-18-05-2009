&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE pi-nf-cancelada B-table-Win 
PROCEDURE pi-nf-cancelada :
/*------------------------------------------------------------------------------
  Purpose:     
  Parameters:  <none>
  Notes:       
------------------------------------------------------------------------------*/
 ASSIGN fi-total-1 = 0
        fi-total-2 = 0
        fi-total-3 = 0
        fi-total-4 = 0.

 FOR EACH nota-fiscal WHERE
          nota-fiscal.dt-emis-nota >= da-dt-entrega-ini   AND 
          nota-fiscal.dt-emis-nota <= da-dt-entrega-fin 
          NO-LOCK
    BREAK BY nota-fiscal.nr-nota-fis.

     RUN pi-acompanhar IN h-acomp (INPUT "Data: "    + STRING(nota-fiscal.dt-emis-nota) +
                                         " Nota Fiscal: " + nota-fiscal.nr-nota-fis).

     IF nota-fiscal.dt-cancela = ? OR
        (nota-fiscal.cod-estabel <> "2" AND nota-fiscal.serie <> "1") THEN NEXT. 

     IF nota-fiscal.nome-ab-cli  < c-nome-abrev-ini OR
        nota-fiscal.nome-ab-cli  > c-nome-abrev-fin  THEN NEXT.
     
     IF nota-fiscal.no-ab-reppri < c-no-ab-reppri-ini OR
        nota-fiscal.no-ab-reppri > c-no-ab-reppri-fin THEN NEXT.

     IF nota-fiscal.nr-pedcli    < c-nr-pedcli-ini OR
        nota-fiscal.nr-pedcli    > c-nr-pedcli-fin THEN NEXT.

     IF l-dt-cancela = YES AND
        (nota-fiscal.dt-cancela < da-dt-cancela-ini OR
         nota-fiscal.dt-cancela > da-dt-cancela-fin ) THEN NEXT.

     FOR EACH it-nota-fisc OF nota-fiscal NO-LOCK.
         ACCUMULATE it-nota-fisc.qt-faturada[1] (TOTAL).
     END.

     ACCUMULATE nota-fiscal.vl-tot-nota (TOTAL).
     ACCUMULATE nota-fiscal.nr-nota-fis (COUNT).

     CREATE tt-work.
     ASSIGN tt-work.nr-nota-fis  = nota-fiscal.nr-nota-fis
            tt-work.dt-emis-nota = nota-fiscal.dt-emis-nota
            tt-work.dt-cancel    = nota-fiscal.dt-cancela
            tt-work.nome-ab-cli  = nota-fiscal.nome-ab-cli 
            tt-work.no-ab-reppri = nota-fiscal.no-ab-reppri   
            tt-work.nr-pedcli    = nota-fiscal.nr-pedcli  
            tt-work.qtd-nota     = ACCUM TOTAL it-nota-fisc.qt-faturada[1]
            tt-work.vl-tot-nota  = nota-fiscal.vl-tot-nota
            tt-work.motivo       = nota-fiscal.desc-cancela.

     ASSIGN fi-total-1 = fi-total-1 + ACCUM TOTAL it-nota-fisc.qt-faturada[1].

 END.
 ASSIGN fi-total-2 = ACCUM TOTAL nota-fiscal.vl-tot-nota
        fi-total-4 = ACCUM COUNT nota-fiscal.nr-nota-fis.
 DISPLAY fi-total-1
         fi-total-2
         fi-total-4 
         WITH FRAME {&FRAME-NAME}.


END PROCEDURE.

