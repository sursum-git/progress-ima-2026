PROCEDURE calcTotal :
/*------------------------------------------------------------------------------
  Purpose:     
  Parameters:  <none>
  Notes:       
------------------------------------------------------------------------------*/

FOR EACH ttEtq.
    ASSIGN dTotal = dTotal + ttEtq.qtEtq.
END.


END PROCEDURE.

