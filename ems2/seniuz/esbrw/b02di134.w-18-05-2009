PROCEDURE pi-relat-dev :
/*------------------------------------------------------------------------------
  Purpose:     
  Parameters:  <none>
  Notes:       
------------------------------------------------------------------------------*/

 DEF INPUT PARAMETER p-tipo-relat AS INT.
 DEF VAR h-prog AS HANDLE NO-UNDO.
 DEF VAR i-ct   AS INT.

 RUN utp/ut-utils.p PERSISTENT SET h-prog.


 RUN esapi/saida-imp.p (OUTPUT i-saida,
                        OUTPUT c-saida,
                        OUTPUT i-num-copias,
                        OUTPUT l-ok).

 CASE i-saida:
     WHEN 1 THEN DO.
         OUTPUT TO PRINTER CONVERT TARGET "ibm850" PAGED PAGE-SIZE 64.
         PUT CONTROL "~033E~033(s18H".    
     END.
     WHEN 2 THEN
         OUTPUT TO VALUE(c-saida).
     WHEN 3 THEN DO.
         ASSIGN c-saida = SESSION:TEMP-DIRECTORY + "essp0183.tmp".
         OUTPUT TO VALUE(c-saida).
     END.
 END CASE.
 DO i-ct = 1 TO i-num-copias.
    ASSIGN i-lin = 99
           i-pag = 1.
    FOR EACH tt-work NO-LOCK 
        BREAK BY tt-work.dt-devol.
        IF i-lin > 64 THEN DO:
           RUN pi-imp-cabec (INPUT p-tipo-relat).
           ASSIGN i-lin = 7.
        END.
        PUT tt-work.nr-nota-fis  FORMAT "x(7)"        AT   1
            tt-work.dt-emis-nota                      AT  10
            tt-work.nome-ab-cli                       AT  21
            tt-work.nr-pedcli    FORMAT "x(6)"        AT  34
            tt-work.no-ab-reppri                      AT  41
            tt-work.qtd-nota                          AT  55
            tt-work.vl-tot-nota  FORMAT "->>>,>>9.99" AT  66
            tt-work.dt-devol                          AT  78
            tt-work.nro-docto    FORMAT "x(7)"        AT  89
            tt-work.qtd-dev                           AT  98
            tt-work.vlr-dev                           AT 109
            tt-work.motivo       FORMAT "x(16)"       AT 120.

        ACCUMULATE tt-work.qtd-nota    (TOTAL).
        ACCUMULATE tt-work.vl-tot-nota (TOTAL).
        ACCUMULATE tt-work.qtd-dev     (TOTAL).
        ACCUMULATE tt-work.vlr-dev     (TOTAL).

        ASSIGN i-lin = i-lin + 1.

    END.
    IF (ACCUM TOTAL tt-work.qtd-nota) > 0 THEN DO:
       IF i-lin > 63 THEN DO:
          PAGE.
          RUN pi-cab-devol.
          ASSIGN i-lin = 7.
       END.
       PUT "----------------------                    ----------------------" AT  55 SKIP.
       PUT ACCUM TOTAL tt-work.qtd-nota    AT  55.
       PUT ACCUM TOTAL tt-work.vl-tot-nota AT  67.
       PUT ACCUM TOTAL tt-work.qtd-dev     AT  98. 
       PUT ACCUM TOTAL tt-work.vlr-dev     AT 109 SKIP.
    END.
    IF i-saida = 1 THEN DO:
        PAGE.
        PUT "" AT 1.
    END.

 END.
 IF i-saida = 3 THEN DO.
    RUN EXECUTE IN h-prog(INPUT "notepad.exe",
                          INPUT c-saida).
    DELETE PROCEDURE h-prog.
 END.
 OUTPUT CLOSE.

END PROCEDURE.

